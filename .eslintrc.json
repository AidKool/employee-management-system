{
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:node/recommended",
    "plugin:promise/recommended"
  ],
  "plugins": ["prettier", "promise"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "func-names": "off",
    "no-use-before-define": "off",
    "no-console": "off"
  }
}
